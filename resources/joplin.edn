{:migrators    {:sql-mig    "/migrations"}

 :seeds        {:sql-seed  "seeds.sql/run"}
 
 :databases    {:psql-dev  {:type :sql, :url #envf ["jdbc:postgresql:playground_dev?user=%s&password=%s" MANANDEARTH_PLAYGROUND_USER MANANDEARTH_PLAYGROUND_PASSWORD]}
                :psql-test {:type :sql, :url #envf ["jdbc:postgresql:playground_test?user=%s&password=%s" MANANDEARTH_PLAYGROUND_USER MANANDEARTH_PLAYGROUND_PASSWORD]}}

 :environments {:dev       [{:db :psql-dev,  :migrator :sql-mig, :seed :sql-seed}]
                :test      [{:db :psql-test, :migrator :sql-mig, :seed :sql-seed}]}
 }
